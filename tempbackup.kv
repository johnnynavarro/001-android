ScreenManager:
    Tela1:
    Tela2:
    Tela3:
    Tela4:
    Tela5:
    Tela6:
    Tela7:

<Tela1>:

    name: 'tela1'
    ScrollView:
    MDFloatLayout:
        #orientation: 'vertical'
        canvas:
            Color:
                rgba: app.theme_cls.primary_color
            Rectangle:
                size: self.width-0, self.height-10
                pos: 0, 900
                #valing: 'top'
        Image:
            orientation: 'vertical'
            size_hint: 0.2, 0.2
            source: 'inop.png'
            pos_hint: {'center_x':0.05, 'center_y': 0.98}

        MDToolbar:
            title: "Dia 15 de novembro teremos eleição para prefeito quem o Sr(a) votaria?"
            pos_hint: {'center_x':0.5, 'center_y': 0.90}

        MDLabel:
            text: 'Júnior Nenzim'
            pos_hint: {'center_x':0.6, 'center_y':0.8}

        MDCheckbox:
            group: 'grupo'
            size_hint: None, None
            size: dp(40), dp (40)
            pos_hint: {'center_x':0.8, 'center_y':0.8}
            on_press:
                value = self.active
                if value: True
                root.prefeito = 'Júnior do Nenzim'
                root.imprimir()
                root.manager.current = 'tela2'


        MDLabel:
            text: 'Rigo Teles'
            pos_hint: {'center_x':0.6, 'center_y':0.7}

        MDCheckbox:
            group: 'grupo'
            size_hint: None, None
            size: dp(40), dp (40)
            pos_hint: {'center_x':0.8, 'center_y':0.7}
            on_release:
                value = self.active
                if value: True
                root.prefeito = 'Rigo Teles'
                root.imprimir()
                root.manager.current = 'tela2'

        MDLabel:
            text: 'Dino Penha'
            pos_hint: {'center_x':0.6, 'center_y':0.6}

        MDCheckbox:
            group: 'grupo'
            size_hint: None, None
            size: dp(40), dp (40)
            pos_hint: {'center_x':0.8, 'center_y':0.6}
            on_release:
#                value = self.active
#                if value: True
#                root.prefeito = 'Dino Penha'
#                root.imprimir()
                root.manager.current = 'tela2'

        MDLabel:
            text: 'Não Sabe/Não Opinou'
            pos_hint: {'center_x':0.6, 'center_y':0.3}

        MDCheckbox:
            group: 'grupo'
            size_hint: None, None
            size: dp(40), dp (40)
            pos_hint: {'center_x':0.8, 'center_y':0.3}
            on_press:
                value = self.active
                if value: True
                root.prefeito = 'Não Sabe/Não Opinou'
                root.imprimir()
                root.manager.current = 'tela2'

        MDFillRoundFlatButton:
            text: 'próximo'
            pos_hint: {'center_x':0.80,'center_y':0.1}
            on_press: root.manager.current = 'tela2'
<Tela2>:
    name: 'tela2'
    MDFloatLayout:
        orientation: 'vertical'
        canvas:
            Color:
                rgba: app.theme_cls.primary_color
            Rectangle:
                size: self.width-0, self.height-10
                pos: 0, 650
                #valing: 'top'
        Image:
            orientation: 'vertical'
            size_hint: None, None
            source: 'inop.png'
            pos_hint: {'center_x':0.05, 'center_y': 0.98}

        MDToolbar:
            title: "Quais os principais problemas de sua localidade(sede e povoados)?"
            pos_hint: {'center_x':0.5, 'center_y': 0.90}

        MDTextField:
            hint_text: "Name"
            mode: "rectangle"
            size_hint: 0.75, None
            icon_right: "arrow-down-drop-circle-outline"
            icon_right_color: app.theme_cls.primary_color
            pos_hint: {'center_x':0.5, 'center_y':0.5}

        MDFillRoundFlatButton:
            text: 'Próximo'
            pos_hint: {'center_x':0.80,'center_y':0.1}
            on_press: root.manager.current = 'tela3'

        MDFillRoundFlatButton:
            text: 'Voltar'
            pos_hint: {'center_x':0.20,'center_y':0.1}
            on_press: root.manager.current = 'tela1'

        MDLabel:
            text: 'Não Sabe/Não Opinou'
            pos_hint: {'center_x':0.7, 'center_y':0.3}

        MDCheckbox:
            group: 'grupo'
            size_hint: None, None
            size: dp(40), dp (40)
            pos_hint: {'center_x':0.8, 'center_y':0.3}
            on_press: root.manager.current = 'tela3'

